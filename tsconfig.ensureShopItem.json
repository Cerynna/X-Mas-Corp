{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022"],
    "module": "ES2022",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "outDir": "scripts/ensureShopItem",
    "strict": true,
    "skipLibCheck": true,
    "types": ["node"]
  },
  "include": ["scripts/ensureShopItem/**/*.ts"],
  "exclude": ["scripts/ensureShopItem/*.js", "scripts/ensureShopItem/**/*.js"]
}
